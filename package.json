{
  "name": "printify-homework-sdet",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "lint": "ng lint",
    "test": "npm-run-all --serial clean-reports test:headless serve-report",
    "test:headless": "cypress run",
    "clean-reports": "rimraf tests/reports",
    "cypress:cli": "cypress open",
    "combine-reports": "mochawesome-merge tests/reports/unmerged/*.json > tests/reports/report.json",
    "generate-report": "marge tests/reports/report.json -f report -o tests/reports/ready --inline",
    "serve-report": "npm-run-all --serial combine-reports generate-report --continue-on-error"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.4",
    "@angular/common": "~9.1.4",
    "@angular/compiler": "~9.1.4",
    "@angular/core": "~9.1.4",
    "@angular/forms": "~9.1.4",
    "@angular/platform-browser": "~9.1.4",
    "@angular/platform-browser-dynamic": "~9.1.4",
    "@angular/router": "~9.1.4",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.4",
    "@angular/cli": "~9.1.4",
    "@angular/compiler-cli": "~9.1.4",
    "@angular/language-service": "~9.1.4",
    "@types/mochawesome": "^6.2.1",
    "@types/node": "^12.20.18",
    "codelyzer": "^5.1.2",
    "cypress": "8.1.0",
    "mocha": "9.0.3",
    "mochawesome": "6.2.2",
    "mochawesome-merge": "4.2.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead",
    "not IE 9-11"
  ]
}
